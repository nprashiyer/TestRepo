{
    "$schema": "http://schema.management.azure.com/schemas/2014-04-01-preview/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "workspacename": {
            "type": "string"
        },
        "location": {
            "type": "string"
        },
        "sku": {
            "type": "string"
        },
        "tags": {
            "type": "object"
        }
    },
    "resources": [
        {
            "type": "Microsoft.OperationalInsights/workspaces",
            "apiVersion": "2017-03-15-preview",
            "name": "[parameters('workspacename')]",
            "location": "[parameters('location')]",
            "tags": "[parameters('tags')]",
            "properties": {
                "sku": {
                    "name": "[parameters('sku')]"
                }
            }
        }
    ]
    "outputs": {
        "provisionState": {
            "type": "string",
            "value": "[reference(resourceId('Microsoft.OperationalInsights/workspaces',parameters('workspacename'))).provisioningState]"
        },
        "workspaceId": {
           "type": "string",
           "value": "[reference(resourceId('Microsoft.OperationalInsights/workspaces',parameters('workspacename'))).customerId]"
        }
    }
}
